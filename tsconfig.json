    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "target": "ESNext",
    "module": "Node16",
    "moduleResolution": "node16"
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
